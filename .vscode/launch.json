{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Port 8080)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "env": {
        "CHROME_EXECUTABLE": "/Applications/Brave Browser.app/Contents/MacOS/Brave Browser"
      },
      "args": [
        "--web-port=8080",
        "--dart-define-from-file=.config/config.development.json"
      ]
    },
    {
      "name": "Flutter Debug",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    }
  ]
}