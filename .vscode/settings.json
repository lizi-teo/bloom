{
  "window.density.editorTabHeight": "default",
  "debug.toolBarLocation": "docked",
  "files.exclude": {
    // ".gitignore": true,
    ".claude": true,
    // "pubspec.yaml": true,
    // ".mcp.json": true,
    ".gemini": true,
    ".idea": true,
    "android": true,
    "ios": true,
    "build": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".metadata": true,
    "analysis_*": true,
    "devtools_*": true,
    "pubspec.lock": true,
    ".dart_tool": true,
    ".cursorignore": true,
    // "README.md": true,
    "macos": true,
    "windows": true,
    "linux": true,
    "test": true,
    "web": true,
    "bloom_app.iml": true
  },
  "dart.lineLength": 999,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      100
    ],
    "editor.selectionHighlight": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "cursor.chat.terminalShowHoverHint": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.codeLens": false,
  "editor.codeLensFontSize": 12,
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.hover.delay": 1000,
  "editor.hover.hidingDelay": 100,
  "editor.insertSpaces": true,
  "editor.lineHeight": 2.4,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 45,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "editor.minimap.sectionHeaderFontSize": 9,
  "editor.minimap.showMarkSectionHeaders": true,
  "editor.minimap.showRegionSectionHeaders": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [],
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.selectionHighlight": false,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.enabled": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "html.format.wrapLineLength": 999,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 12,
  "window.confirmSaveUntitledWorkspace": false,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorBracketMatch.border": "#2a2a2a",
    "tab.activeBackground": "#ff9cef48",
    "tab.activeBorder": "#ff9cee7f",
    "tab.activeBorderTop": "#ff9cee7f",
    "minimap.background": "#1a1a1a",
    "minimap.errorHighlight": "#fd0303",
    "minimap.findMatchHighlight": "#c4abab5b",
    "minimap.foregroundOpacity": "#c4abab5b",
    "minimap.selectionHighlight": "#c4abab5b",
    "minimap.selectionOccurrenceHighlight": "#c4abab5b",
    "minimap.warningHighlight": "#ff0000",
    "minimapGutter.addedBackground": "#ff0000",
    "minimapGutter.deletedBackground": "#2bff00",
    "minimapGutter.modifiedBackground": "#0011ff",
    "minimapSlider.activeBackground": "#ffffff2d",
    "minimapSlider.background": "#ffffff2d",
    "minimapSlider.hoverBackground": "#ffffff2d",
    "editor.background": "#1a1a1a",
    "editor.lineHighlightBackground": "#0059ff42",
    "editor.selectionBackground": "#ffffff27",
    "editor.findMatchBackground": "#002fff96",
    "editor.findMatchHighlightBackground": "#ffffff1c",
    "editor.findRangeHighlightBackground": "#ff1e0077",
    "editorWhitespace.foreground": "#6666664d",
    "editorIndentGuide.background1": "#6666664d",
    "editorIndentGuide.activeBackground1": "#fbff014d",
    "editorIndentGuide.background2": "#12e6834d",
    "editorIndentGuide.activeBackground2": "#4f28b94d"
  },
  "workbench.editor.enablePreview": false,
  "workbench.tree.indent": 24,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.activityBar.orientation": "vertical",
  "editor.showFoldingControls": "never",
  "material-icon-theme.hidesExplorerArrows": false,
  "workbench.tree.enableStickyScroll": false,
  "explorer.compactFolders": false,
  "workbench.activityBar.location": "top",
  "files.eol": "\n",
  // left side of the editor gutter line numbers
  // "editor.glyphMargin": false,
  // right side of the editor gutter line numbers
  "editor.lineDecorationsWidth": 18,
  "dart.flutterRunAdditionalArgs": [
    "--dart-define-from-file=.config/config.development.json"
  ],
  "dart.env": {
    "CHROME_EXECUTABLE": "/Applications/Brave Browser.app/Contents/MacOS/Brave Browser"
  },
  "dart.flutterSelectDeviceWhenConnected": false,
  "dart.defaultLaunchTarget": "chrome",
}